{
  "name": "media-handling",
  "version": "1.0.0",
  "description": "application for demoing CAP media handling",
  "repository": "<Add your repository here>",
  "license": "WTFPL",
  "dependencies": {
    "@sap/cds": "^4.1.9",
    "@sap/cds-odata-v2-adapter-proxy": "^1.4.47",
    "@ui5/cli": "2.4.5",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "passport": "^0.4.1"
  },
  "engines": {
    "node": "^10"
  },
  "scripts": {
    "build": "cds build/all --clean",
    "deploy": "cds deploy",
    "start": "cds run"
  },
  "files": [
    "srv",
    "db"
  ],
  "cds": {
    "odata": {
      "version": "v4"
    },
    "requires": {
      "db": {
        "kind": "sqlite",
        "model": [
          "srv",
          "db"
        ],
        "credentials": {
          "database": "media-handling.db"
        }
      }
    }
  },
  "devDependencies": {
    "sqlite3": "^5.0.0"
  }
}
